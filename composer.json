{
    "name": "fahu/sicherer",
    "description": "A CLI tool to create backups of web applications",
    "keywords": ["backup"],
    "homepage": "https://fahu.at",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Hurnaus",
            "email": "me@fahu.at"
        }
    ],
    "require": {
        "php": "^8.1",
        "ifsnop/mysqldump-php": "^2.9",
        "laravel-zero/framework": "^8.4",
        "ext-zip": "*",
        "microsoft/azure-storage-blob": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["sicherer"]
}
